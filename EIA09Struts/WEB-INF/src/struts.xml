<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />
	
		<package name="eia" namespace="/eia" extends="struts-default">
			<action name="showMenu">
				<result>/pages/mainMenu.jsp</result>
			</action>
			<action name="showProjectDescription">
				<result>/pages/proyecto/menuProyecto.jsp</result>
			</action>	
			<action name="showAlternativaInfo">
				<result>/pages/alternativa/alternativaInfo.jsp</result>
			</action>				
			<action name="doLoadProject" class="eia.load.LoadProject">
				<result name="input">/pages/mainMenu.jsp</result>
				<result name="error">/pages/mainMenu.jsp</result>
				<result>/pages/mainMenu.jsp</result>
			</action>					
		</package>
	<!-- Add packages here -->

</struts>
